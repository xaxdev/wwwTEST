{
    "parser": "babel-eslint",
    "env": {
        "es6": true
    },
    "rules": {
        "no-console": 0,
        "new-cap": 0,
        "strict": 1,
        "no-underscore-dangle": 0,
        "no-use-before-define": 0,
        "eol-last": 0,
        "quotes": [2, "single"],
        "jsx-quotes": [2, "prefer-double"]
    },
    "parserOptions": {
          "ecmaVersion": 6,
          "sourceType": "module",
          "ecmaFeatures": {
            "modules": true
          }
    }
}
