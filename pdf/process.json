{
  "apps" : [{
    "name": "pdf",
    "script": "./bootstrap.js",
    "node_args": "--max_old_space_size=2048",
    "watch": false,
    "env": {
      "NODE_ENV": "production",
    },
    "merge_logs": true,
    "error_file": "./logs/pdf-err.log",
    "out_file": "./logs/pdf-out.log",
    "log_date_format": "YYYY-MM-DD HH:mm Z"
  }]
}
